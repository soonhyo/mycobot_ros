<?xml version="1.0"?>
<robot  xmlns:xacro="http://www.ros.org/wiki/xacro" name="mycobot" >
  <xacro:include filename="$(find mycobot_description)/urdf/common.xacro" />

  <link name="link1">
    <inertial>
      <origin xyz="0 0 0.035" rpy="0 0 0" />
      <mass value="10.0" />
      <xacro:cylinder_inertia m="10.0" r="0.05" h="0.07" />
    </inertial>
    <visual>
      <geometry>
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link1.obj"/>
      </geometry>
      <origin xyz = "0.0 0 0 " rpy = " 0 0 -${pi/2}"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link1.obj"/>
        </geometry>
        <origin xyz = "0.0 0 0 " rpy = " 0 0 -${pi/2}"/>
    </collision>
  </link>

  <link name="link2">
