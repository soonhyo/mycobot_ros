      <xacro:cylinder_inertia m="0.4" r="0.05" h="0.155" />
    </inertial>
    <visual>
      <geometry>
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link10.dae"/>
      </geometry>
    <origin xyz = "0.0 0 0.03256 " rpy = " 0 -${pi/2} 0"/>
    </visual>
    <collision>
      <geometry>
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link10.dae"/>
      </geometry>
      <origin xyz = "0.0 0 0.03256 " rpy = " 0 -${pi/2} 0"/>
    </collision>
  </link>

  <link name="link11">
    <inertial>
      <origin xyz="-0.01 0 0.05" rpy="0 0 0" />
