       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link13.dae"/>
      </geometry>
    <origin xyz = "0 0.00 -0.038 " rpy = " 0 0 0"/>
    </visual>
    <collision>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link13.dae"/>
      </geometry>
    <origin xyz = "0 0.00 -0.038 " rpy = " 0 0 0"/>
    </collision>
  </link>

  <link name="link14">
    <inertial>
      <origin xyz="0 0.0025 0" rpy="${M_PI/2} 0 0" />
      <mass value="0.1" />
      <xacro:cylinder_inertia m="0.1" r="0.02" h="0.005" />
    </inertial>
    <visual>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link14.dae"/>
      </geometry>
    <origin xyz = "0.0 0 -0.012 " rpy = " 0 0 0"/>
    </visual>
    <collision>
      <geometry>
       <!--- 0.0 0 -0.04 -->
       <mesh filename="package://mycobot_description/urdf/mycobot/mesh/link14.dae"/>
      </geometry>
    <origin xyz = "0.0 0 -0.012 " rpy = " 0 0 0"/>
    </collision>
  </link>

  <joint name="joint7" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "10.0" lower = "${-pi}" upper = "${pi}" velocity = "1.0"/>
    <parent link="link8"/>
    <child link="link9"/>
    <origin xyz= "0 0 0.13156" rpy = "0 0 0"/>
    <dynamics damping="1.0e-6" friction="2.0e-1" />
  </joint>

  <joint name="joint8" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "10.0" lower = "${-pi}" upper = "${pi}" velocity = "1.0"/>
    <parent link="link9"/>
    <child link="link10"/>
    <origin xyz= "0 0  0" rpy = "0 ${pi/2} -${pi/2}"/>
    <dynamics damping="1.0e-6" friction="2.0e-1" />
  </joint>

  <joint name="joint9" type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "10.0" lower = "${-pi}" upper = "${pi}" velocity = "1.0"/>
    <parent link="link10"/>
    <child link="link11"/>
    <origin xyz= "-0.1104 0 0   " rpy = "0 0 0"/>
    <dynamics damping="1.0e-6" friction="2.0e-1" />
  </joint>

  <joint name="joint10" Type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "10.0" lower = "${-pi}" upper = "${pi}" velocity = "1.0"/>
    <parent link="link11"/>
    <child link="link12"/>
    <origin xyz= "-0.096 0 0.06462" rpy = "0 0 -${pi/2}"/>
    <dynamics damping="1.0e-6" friction="2.0e-1" />
  </joint>

  <joint name="joint11" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "10.0" lower = "${-pi}" upper = "${pi}" velocity = "1.0"/>
    <parent link="link12"/>
    <child link="link13"/>
    <origin xyz= "0 -0.07318 0" rpy = "${pi/2} -${pi/2} 0"/>
    <dynamics damping="1.0e-6" friction="2.0e-1" />
  </joint>

  <joint name="joint12" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "10.0" lower = "${-pi}" upper = "${pi}" velocity = "1.0"/>
    <parent link="link13"/>
    <child link="link14"/>
    <origin xyz= "0 0.0456 0" rpy = "-${pi/2} 0 0"/>
    <dynamics damping="1.0e-6" friction="2.0e-1" />
  </joint>

  <transmission name="joint7_trans">
    <provideFeedback>true</provideFeedback>
    <implicitSpringDamper>0</implicitSpringDamper>
    <motorTorqueConstant>0.418</motorTorqueConstant>
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint7">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint7_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <transmission name="joint8_trans">
    <provideFeedback>true</provideFeedback>
    <implicitSpringDamper>0</implicitSpringDamper>
    <motorTorqueConstant>0.418</motorTorqueConstant>
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint8">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
